[
  {
    "title": "Linode",
    "url": "https://www.linode.com/speedtest",
    "location": ".speedtest-table td:nth-child(1)",
    "host": ".speedtest-table td:nth-child(2)"
  },
  {
    "title": "Vultr",
    "url": "https://www.vultr.com/faq/",
    "location": ".table980 td:nth-child(1)",
    "host": ".table980 td:nth-child(2)"
  },
  {
    "title": "DigitalOcean",
    "url": "http://speedtest-sfo1.digitalocean.com/",
    "location": "dt a",
    "host": "dt",
    "host_replacements":{
      "<a href=\"http://(.+)\">.+</a>": "<a>$1</a>"
    }
  },
  {
    "title": "SoftLayer",
    "url": "http://www.softlayer.com/data-centers",
    "location": ".col1.ssl-right-bord",
    "location_replacements": {
      "<br>": ", "
    },
    "host": ".col7.ssl-bot-bord",
    "excludes": ["speedtest.dal02.softlayer.com"]
  },
  {
    "title": "VPS.to",
    "url": "https://vps.to/speedtest/",
    "location": ".speedtest td:nth-child(1)",
    "host": ".speedtest td:nth-child(2)"
  },
  {
    "title": "VPS.NET",
    "url": "http://www.cloudtestfiles.net/js/global.js",
    "parser": "regexp",
    "location": ": '([A-Z ]+ - .+?)'",
    "host": ": '(\\d+\\.\\d+\\.\\d+\\.\\d+)'"
  },
  {
    "title":"Atlantic.Net",
    "url":"https://www.atlantic.net/speed-test/",
    "location": "td:nth-child(1)",
    "host": "td:nth-child(3)"
  },
  {
    "title":"BuyVM",
    "url":"http://lv-test.buyvm.net/",
    "parser": "lookingglass"
  },
  {
    "title":"Azure",
    "url":"http://www.azurespeed.com/bundles/azurespeed",
    "parser":"regexp",
    "location":"region:\"(.+?)\"",
    "host": "storage:\"(\\w+)\"",
    "host_replacements": {
      "^((?:(?!china).)*)$": "$1.blob.core.windows.net",
      "^(.*china.*)$": "$1.blob.core.chinacloudapi.cn"
    },
    "excludes": ["cdn.blob.core.windows.net"]
  }
]